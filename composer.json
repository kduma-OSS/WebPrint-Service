{
    "name": "kduma/webprint-service-cli",
    "description": "WebPrint Local Host Service",
    "keywords": ["webprint", "service", "cli"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Krystian Duma",
            "email": "git@krystian.duma.sh"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-zlib": "*"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.2",
        "pestphp/pest": "^1.0",
        "guzzlehttp/guzzle": "^6.5.5",
        "illuminate/http": "^8.0",
        "kduma/lpd": "^0.1.0",
        "laravel-zero/framework": "^8.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["builds/webprint-service"]
}
